





# Uncomment the next line to define a global platform for your project
# modularization_swift
target 'modularization_swift' do 
  workspace 'Untitled_swift.xcworkspace'
  project 'modularization_swift/modularization_swift.xcodeproj'
  platform :ios, '10.0'
  inhibit_all_warnings!
  use_frameworks!

  pod 'AFNetworking'


end 



